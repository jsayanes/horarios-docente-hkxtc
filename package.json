{
  "name": "horarios-docente",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node backend/server-simple.js",
    "backend": "node backend/server-simple.js",
    "dev": "concurrently \"npm run backend\" \"npm run start\"",
    "setup-db": "node backend/scripts/setup-database.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:horarios-docente": "node dist/horarios-docente/server/server.mjs",
    "backend-mysql": "node backend/server.js",
    "dev-mysql": "concurrently \"npm run backend-mysql\" \"npm run start\"",
    "vercel-build": "ng build --configuration production"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.0.2",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/ssr": "^20.0.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "mysql2": "^3.14.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "concurrently": "^9.1.0",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
